package br.com.sicredi.associado.services;

import java.time.Instant;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import br.com.pronetdigital.sicredi.dtos.PautaDTO;
import br.com.pronetdigital.sicredi.entities.Pauta;
import br.com.sicredi.associado.repositories.AssociadoRepository;

@Service
public class AssociadoService {

	@Autowired
	private AssociadoRepository associadoRepository;
	
	public Asssociado insert(AssociadoDTO objDto) {
		Asssociado obj = new Asssociado();
		obj = pautaRepository.save(obj);
		PautaDTO result = new PautaDTO(obj.getId(), obj.getNome());
		return result;
	}

}
